{
  "env": {
    "es6": true,
    "node": true,
    "browser": true
  },
  "overrides": [
    {
      "env": {
        "node": true,
        "cypress": true
      }
    }
  ],
  "extends": ["eslint:recommended", "plugin:prettier/recommended", "plugin:@typescript-eslint/recommended"],
  "rules": {
    "max-depth": ["warn", 2],
    "max-lines-per-function": ["warn", { "max": 20 }]
  }
}
